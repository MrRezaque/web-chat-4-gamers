<div class="game-select-menu-wrap">
    <div class="game-select-menu">
        <div class="game-select">
            <div class="menu-placeholder"></div>
            <div class="thumb-wrapper">
                <div class="-thumbnail  menu-thumb">
                    <%=link_to  csgo_chat_path, class: 'hoverable' do %>
                      <img class="hover-none" src="assets/game_select/csgo_logo.png" alt=""/>
                      <img class="hover" src="assets/game_select/csgo_logo_hover.png" alt=""/>
                    <% end %>
                </div>
                <div class="-thumbnail menu-thumb">
                    <%= link_to  dota2_chat_path, class: 'hoverable' do %>
                      <img class="hover-none" src="assets/game_select/dota_logo.png" alt=""/>
                      <img class="hover" src="assets/game_select/dota_logo_hover.png" alt=""/>
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>